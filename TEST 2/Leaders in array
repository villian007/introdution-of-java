

public class Solution {

	public static void leaders(int[] input) {
        for (int i=0;i<input.length;i++)
        {
            boolean check=true;
            for (int j=i+1;j<input.length;j++)
            {
                if (input[j]>input[i])
                {
                    check=false;
                    break;
                }
            }
            if (check==true)
            {
                System.out.print(input[i] + " ");
            }
        }
	}
	
}
		
	
