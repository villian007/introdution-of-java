import java.util.Scanner;

public class Solution {
    
    public static void main(String[] args) {
      Scanner s = new Scanner(System.in);
      char ch1;
      ch1= s.next().charAt(0);
        
        
        
        
     if(ch1 >=65  && ch1 <=90)
     {System.out.println("1");
      }
        
        
     else if(ch1  >=97 &&  ch1<=122)
     {System.out.println("0");
     }
     else
     {System.out.println("-1");
     }
     
      

    }
}
