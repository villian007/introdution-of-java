import java.util.Scanner;
public class Solution {


	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
        int N=scan.nextInt();
        for (int i=0;i<N;i++)
        {
            for (int j=0;j<=i;j++)
            {
                System.out.print(i+1);
            }
            System.out.println(" ");
        }
		    scan.close();
	}

}
